xcopy "$(ProjectDir)..\sharedModules\SQLite\x86" "$(ProjectDir)..\$(ConfigurationName)\lib\x86" /y /s /q /i
xcopy "$(ProjectDir)..\sharedModules\SQLite\x64" "$(ProjectDir)..\$(ConfigurationName)\lib\x64" /y /s /q /i</PostBuildEvent>
