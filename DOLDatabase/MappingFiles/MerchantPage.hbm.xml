<?xml version="1.0" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.0">
	<class name="DOL.Database.MerchantPage, GameServer" table="`merchantpage`">
		<cache usage="nonstrict-read-write"/>
		<id name="MerchantPageID" type="Int32" column="`MerchantPageID`" unsaved-value="0">
			<generator class="native" />
		</id>
		<property name="Currency" type="DOL.Database.eCurrencyType, GameServer" not-null="true" column="`Currency`"/>
		<property name="Position" type="Int32" not-null="true" column="`Position`"/>
		
		<map name="MerchantItems" cascade="all" table="`MerchantItems`">
			<key column="`MerchantPageID`"/>
			<index column="`Position`" type="Int32"/>
			<one-to-many class="DOL.Database.MerchantItem, GameServer"/>
		</map>
	</class>
</hibernate-mapping>
