<?xml version="1.0" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.0">
	<class name="DOL.Database.GenericItemTemplate, GameServer" table="`itemtemplate`">
		<cache usage="nonstrict-read-write"/>
		<id name="ItemTemplateID" type="String(255)" column="`ItemTemplateID`">
			<generator class="assigned" />
		</id>
		<discriminator column="ItemTemplateType"/> 
		
		<property name="Name" type="String(255)" not-null="true" column="`Name`"/>
		<property name="Level" type="Byte" not-null="true" column="`Level`"/>
		<property name="Weight" type="Int32" not-null="true" column="`Weight`"/>
		<property name="Value" type="Int64" not-null="true" column="`Value`"/>
		<property name="Realm" not-null="true" column="`Realm`"/>
		<property name="Model" type="Int32" not-null="true" column="`Model`"/>
		<property name="IsSaleable" type="YesNo" not-null="true" column="`IsSaleable`"/>
		<property name="IsTradable" type="YesNo" not-null="true" column="`IsTradable`"/>
		<property name="IsDropable" type="YesNo" not-null="true" column="`IsDropable`"/>
		
		<subclass name="DOL.Database.TravelTicketTemplate, GameServer">
			<property name="TripPathID" type="Int32" not-null="true" column="`TripPathID`"/> 
		</subclass>
		<subclass name="DOL.Database.CraftingToolTemplate, GameServer">
			<property name="Type" not-null="true" column="`Type`" /> 
		</subclass>
		<subclass name="DOL.Database.RespecStoneTemplate, GameServer">
			<property name="RespecType" not-null="true" column="`RespecType`" /> 
		</subclass>
		<subclass name="DOL.Database.RenewalTemplate, GameServer" />
		<subclass name="DOL.Database.DyeTemplate, GameServer">
			<property name="Color" type="Int32" not-null="true" column="`Color`" /> 
			<subclass name="DOL.Database.WeaponLusterTemplate, GameServer" />
			<subclass name="DOL.Database.ClothDyeTemplate, GameServer" />
			<subclass name="DOL.Database.EnamelTemplate, GameServer" />
			<subclass name="DOL.Database.LeatherDyeTemplate, GameServer" />
		</subclass>
		<subclass name="DOL.Database.MagicalDustTemplate, GameServer">
			<property name="Bonus" type="Int32" not-null="true" column="`Bonus`" /> 
		</subclass>
		<subclass name="DOL.Database.SpellCraftGemTemplate, GameServer">
			<property name="Quality" type="Int32" not-null="true" column="`Quality`" /> 
			<property name="BonusType" not-null="true" column="`BonusType`" />
			<property name="Bonus" type="Byte" not-null="true" column="`Bonus`" />  
		</subclass>
		<subclass name="DOL.Database.AlchemieTinctureTemplate, GameServer">
			<property name="SpellID" type="Int32" not-null="true" column="`SpellID`" />
			<subclass name="DOL.Database.ActiveTinctureTemplate, GameServer" />
			<subclass name="DOL.Database.ReactiveTinctureTemplate, GameServer" />
			<subclass name="DOL.Database.StableTinctureTemplate, GameServer">
				<property name="Charge" type="Byte" not-null="true" column="`Charge`" /> 
				<property name="MaxCharge" type="Byte" not-null="true" column="`MaxCharge`" /> 
			</subclass>
		</subclass>
    <subclass name="DOL.Database.MagicalItemTemplate, GameServer">
      <property name="Charge" type="Byte" not-null="true" column="`Charge`" />
      <property name="MaxCharge" type="Byte" not-null="true" column="`MaxCharge`" />
      <property name="SpellID" type="Int32" not-null="true" column="`SpellID`" />
      <subclass name="DOL.Database.PotionTemplate, GameServer" />
    </subclass>
    <subclass name="DOL.Database.StackableItemTemplate, GameServer">
			<property name="MaxCount" type="Int32" not-null="true" column="`MaxCount`" />
			<property name="PackSize" type="Int32" not-null="true" column="`PackSize`" />
			<subclass name="DOL.Database.AmmunitionTemplate, GameServer">
				<property name="Precision" not-null="true" column="`Precision`" /> 
				<property name="Damage" not-null="true" column="`Damage`" /> 
				<property name="Range" not-null="true" column="`Range`" />
				<property name="DamageType" not-null="true" column="`DamageType`" />
				<subclass name="DOL.Database.ArrowTemplate, GameServer" />
				<subclass name="DOL.Database.BoltTemplate, GameServer" />
			</subclass>
      <subclass name="DOL.Database.MagicalStackableItemTemplate, GameServer">
        <property name="SpellID" type="Int32" not-null="true" column="`SpellID`" />
        <subclass name="DOL.Database.CatapultAmmunitionTemplate, GameServer" />
        <subclass name="DOL.Database.PoisonTemplate, GameServer" />
      </subclass>
		</subclass>
		<subclass name="DOL.Database.EquipableItemTemplate, GameServer">
			<property name="Quality" type="Byte" not-null="true" column="`Quality`" /> 
			<property name="Bonus" type="Byte" not-null="true" column="`Bonus`" /> 
			<property name="Durability" type="Byte" not-null="true" column="`Durability`" /> 
			<property name="Condition" type="Double" not-null="true" column="`Condition`" /> 
			<property name="MaterialLevel" not-null="true" column="`MaterialLevel`" /> 
			<property name="ProcSpellID" type="Int32" not-null="true" column="`ProcSpellID`" /> 
			<property name="ProcEffectType" not-null="true" column="`ProcEffectType`" /> 
			<property name="ChargeSpellID" type="Int32" not-null="true" column="`ChargeSpellID`" /> 
			<property name="ChargeEffectType" not-null="true" column="`ChargeEffectType`" /> 
			<property name="Charge" type="Byte" not-null="true" column="`Charge`" /> 
			<property name="MaxCharge" type="Byte" not-null="true" column="`MaxCharge`" />
			<set name="MagicalBonus" table="itemmagicalbonustemplate" cascade="all">
				<key column="ItemTemplateID" />
				<composite-element class="DOL.Database.ItemMagicalBonus, GameServer">
					<property name="BonusType" not-null="true" column="`BonusType`"/>
					<property name="Bonus" type="Int16" not-null="true" column="`Bonus`"/>
				</composite-element>
			</set>
			<set name="AllowedClass" table="itemallowedclasstemplate" cascade="all">
				<key column="ItemTemplateID" />
				<element column="AllowedClass" type="DOL.Database.eCharacterClass, GameServer"/>
			</set>
			<subclass name="DOL.Database.JewelTemplate, GameServer" />
			<subclass name="DOL.Database.BracerTemplate, GameServer" />
			<subclass name="DOL.Database.RingTemplate, GameServer" />
			<subclass name="DOL.Database.WaistTemplate, GameServer" />
			<subclass name="DOL.Database.NecklaceTemplate, GameServer" >
				<subclass name="DOL.Database.TeleportationMedaillonTemplate, GameServer">
					<property name="X" type="Int32" not-null="true" column="`X`" /> 
					<property name="Y" type="Int32" not-null="true" column="`Y`" /> 
					<property name="Z" type="Int32" not-null="true" column="`Z`" /> 
					<property name="Heading" type="Int32" not-null="true" column="`Heading`" /> 
					<property name="Region" type="Int32" not-null="true" column="`Region`" /> 
				</subclass>
			</subclass>
			<subclass name="DOL.Database.VisibleEquipmentTemplate, GameServer" >
				<property name="Color" type="Int32" not-null="true" column="`Color`" /> 
				<subclass name="DOL.Database.CloakTemplate, GameServer" />
				<subclass name="DOL.Database.InstrumentTemplate, GameServer">
					<property name="Type" not-null="true" column="`Type`" /> 
				</subclass>
				<subclass name="DOL.Database.ArmorTemplate, GameServer">
					<property name="ArmorFactor" type="Byte" not-null="true" column="`ArmorFactor`" /> 
					<property name="ArmorLevel" not-null="true" column="`ArmorLevel`" />
					<property name="ModelExtension" type="Byte" not-null="true" column="`ModelExtension`" /> 
					<subclass name="DOL.Database.HeadArmorTemplate, GameServer" />
					<subclass name="DOL.Database.TorsoArmorTemplate, GameServer" />
					<subclass name="DOL.Database.HandsArmorTemplate, GameServer" />
					<subclass name="DOL.Database.FeetArmorTemplate, GameServer" />
					<subclass name="DOL.Database.LegsArmorTemplate, GameServer" />
					<subclass name="DOL.Database.ArmsArmorTemplate, GameServer" />
				</subclass>
				<subclass name="DOL.Database.WeaponTemplate, GameServer">
					<property name="DamagePerSecond" type="Byte" not-null="true" column="`DamagePerSecond`" /> 
					<property name="Speed" type="Int32" not-null="true" column="`Speed`" />
					<property name="DamageType" not-null="true" column="`DamageType`" />
					<property name="HandNeeded" not-null="true" column="`HandNeeded`" />
					<property name="GlowEffect" type="Int32" not-null="true" column="`GlowEffect`" />
					<subclass name="DOL.Database.ShieldTemplate, GameServer">
						<property name="Size" not-null="true" column="`Size`" /> 
					</subclass>
					<subclass name="DOL.Database.RangedWeaponTemplate, GameServer">
						<property name="WeaponRange" type="Int32" not-null="true" column="`WeaponRange`" /> 
						<subclass name="DOL.Database.CompositebowTemplate, GameServer" />
						<subclass name="DOL.Database.CrossbowTemplate, GameServer" />
						<subclass name="DOL.Database.LongbowTemplate, GameServer" />
						<subclass name="DOL.Database.RecurvedbowTemplate, GameServer" />
						<subclass name="DOL.Database.ShortbowTemplate, GameServer" />
						<subclass name="DOL.Database.ThrownWeaponTemplate, GameServer" >
							<property name="MaxCount" type="Int32" not-null="true" column="`MaxCount`" />
							<property name="PackSize" type="Int32" not-null="true" column="`PackSize`" />
						</subclass>
					</subclass>
					<subclass name="DOL.Database.AxeTemplate, GameServer" />
					<subclass name="DOL.Database.BladesTemplate, GameServer" />
					<subclass name="DOL.Database.BluntTemplate, GameServer" />
					<subclass name="DOL.Database.CelticSpearTemplate, GameServer" />
					<subclass name="DOL.Database.CrushingWeaponTemplate, GameServer" />
					<subclass name="DOL.Database.FlexibleWeaponTemplate, GameServer" />
					<subclass name="DOL.Database.HammerTemplate, GameServer" />
					<subclass name="DOL.Database.HandToHandTemplate, GameServer" />
					<subclass name="DOL.Database.LargeWeaponTemplate, GameServer" />
					<subclass name="DOL.Database.LeftAxeTemplate, GameServer" />
					<subclass name="DOL.Database.PiercingTemplate, GameServer" />
					<subclass name="DOL.Database.PolearmWeaponTemplate, GameServer" />
					<subclass name="DOL.Database.ScytheTemplate, GameServer" />
					<subclass name="DOL.Database.SlashingWeaponTemplate, GameServer" />
					<subclass name="DOL.Database.SpearTemplate, GameServer" />
					<subclass name="DOL.Database.StaffTemplate, GameServer" />
					<subclass name="DOL.Database.SwordTemplate, GameServer" />
					<subclass name="DOL.Database.ThrustWeaponTemplate, GameServer" />
					<subclass name="DOL.Database.TwoHandedWeaponTemplate, GameServer" />
				</subclass>	
			</subclass>
		</subclass>
	</class>
</hibernate-mapping>
