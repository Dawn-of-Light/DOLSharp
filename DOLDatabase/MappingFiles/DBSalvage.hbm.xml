<?xml version="1.0" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.0">
	<class name="DOL.Database.DBSalvage, GameServer" table="`salvage`">
		<id name="SalvageID" type="Int32" column="`SalvageID`" unsaved-value="0">
			<generator class="native" />
		</id>
		<property name="ObjectType" type="Int32" not-null="true" column="`ObjectType`"/>
		<property name="SalvageLevel" type="Int32" not-null="true" column="`SalvageLevel`"/>
		<many-to-one name="MaterialItemtemplate" not-null="true" class="DOL.Database.StackableItemTemplate, GameServer" column="`MaterialItemtemplate`"/>	
	</class>
</hibernate-mapping>
