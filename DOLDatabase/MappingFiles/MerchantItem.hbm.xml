<?xml version="1.0" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.0">
	<class name="DOL.Database.MerchantItem, GameServer" table="`merchantitem`">
		<cache usage="nonstrict-read-write"/>
		<id name="MerchantItemID" type="Int32" column="`MerchantItemID`" unsaved-value="0">
			<generator class="native" />
		</id>
		<property name="Position" type="Int32" not-null="true" column="`Position`"/>
		<many-to-one name="ItemTemplate" cascade="none" class="DOL.Database.GenericItemTemplate, GameServer" column="`PersistantGameObjectID`"/>
	</class>
</hibernate-mapping>
