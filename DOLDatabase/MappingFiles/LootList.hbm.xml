<?xml version="1.0" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.0">
	<class name="DOL.Database.Loot.LootList, GameServer" table="`lootlist`">
		<cache usage="nonstrict-read-write"/>
		<id name="LootListID" type="Int32" column="`LootListID`" unsaved-value="0">
			<generator class="native" />
		</id>
		<set name="AllLoots" inverse="true" cascade="all-delete-orphan">
			<key column="LootListID" />
			<one-to-many class="DOL.Database.Loot.BaseLoot, GameServer"/>
		</set>
	</class>
</hibernate-mapping>
