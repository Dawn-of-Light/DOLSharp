<?xml version="1.0" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.0">
	<class name="DOL.Database.GenericItem, GameServer" table="`item`">
		<id name="ItemID" column="`ItemID`" unsaved-value="0">
			<generator class="native" />
		</id>
		<discriminator column="GenericItemType"/> 
		
		<property name="Name" type="String(255)" not-null="true" column="`Name`"/>
		<property name="Level" type="Byte" not-null="true" column="`Level`"/>
		<property name="Weight" type="Int32" not-null="true" column="`Weight`"/>
		<property name="Value" type="Int64" not-null="true" column="`Value`"/>
		<property name="Realm" not-null="true" column="`Realm`"/>
		<property name="Model" type="Int32" not-null="true" column="`Model`"/>
		<property name="IsSaleable" type="YesNo" not-null="true" column="`IsSaleable`"/>
		<property name="IsTradable" type="YesNo" not-null="true" column="`IsTradable`"/>
		<property name="IsDropable" type="YesNo" not-null="true" column="`IsDropable`"/>
		<property name="QuestName" type="String(255)" not-null="true" column="`QuestName`"/>
		<property name="CrafterName" type="String(255)" not-null="true" column="`CrafterName`"/>
		<property name="SlotPosition" type="Int32" not-null="true" column="`SlotPosition`"/>
		
		<many-to-one name="Owner" class="DOL.Database.GamePlayer, GameServer" not-null="true" column="`Owner`"/>
		
		<subclass name="DOL.Database.TravelTicket, GameServer">
			<property name="TripPathID" type="Int32" not-null="true" column="`TripPathID`" /> 
		</subclass>
		<subclass name="DOL.Database.CraftingTool, GameServer">
			<property name="Type" not-null="true" column="`Type`" /> 
		</subclass>
		<subclass name="DOL.Database.RespecStone, GameServer">
			<property name="RespecType" not-null="true" column="`RespecType`" /> 
		</subclass>
		<subclass name="DOL.Database.Renewal, GameServer" />
		<subclass name="DOL.Database.Dye, GameServer">
			<property name="Color" type="Int32" not-null="true" column="`Color`" /> 
			<subclass name="DOL.Database.WeaponLuster, GameServer" />
			<subclass name="DOL.Database.ClothDye, GameServer" />
			<subclass name="DOL.Database.Enamel, GameServer" />
			<subclass name="DOL.Database.LeatherDye, GameServer" />
		</subclass>
		<subclass name="DOL.Database.MagicalDust, GameServer">
			<property name="Bonus" type="Int32" not-null="true" column="`Bonus`" /> 
		</subclass>
		<subclass name="DOL.Database.SpellCraftGem, GameServer">
			<property name="Quality" type="Int32" not-null="true" column="`Quality`" /> 
			<property name="BonusType" not-null="true" column="`BonusType`" />
			<property name="Bonus" type="Byte" not-null="true" column="`Bonus`" />  
		</subclass>
		<subclass name="DOL.Database.AlchemieTincture, GameServer">
			<property name="SpellID" type="Int32" not-null="true" column="`SpellID`" />
			<subclass name="DOL.Database.ActiveTincture, GameServer" />
			<subclass name="DOL.Database.ReactiveTincture, GameServer" />
			<subclass name="DOL.Database.StableTincture, GameServer">
				<property name="Charge" type="Byte" not-null="true" column="`Charge`" /> 
				<property name="MaxCharge" type="Byte" not-null="true" column="`MaxCharge`" /> 
			</subclass>
		</subclass>
    <subclass name="DOL.Database.MagicalItem, GameServer">
      <property name="Charge" type="Byte" not-null="true" column="`Charge`" />
      <property name="MaxCharge" type="Byte" not-null="true" column="`MaxCharge`" />
      <property name="SpellID" type="Int32" not-null="true" column="`SpellID`" />
      <subclass name="DOL.Database.Potion, GameServer" />
    </subclass>
    
    <subclass name="DOL.Database.StackableItem, GameServer">
			<property name="MaxCount" type="Int32" not-null="true" column="`MaxCount`" />
			<property name="Count" type="Int32" not-null="true" column="`Count`" />
			<subclass name="DOL.Database.Ammunition, GameServer">
				<property name="Precision" not-null="true" column="`Precision`" /> 
				<property name="Damage" not-null="true" column="`Damage`" /> 
				<property name="Range" not-null="true" column="`Range`" />
				<property name="DamageType" not-null="true" column="`DamageType`" />
				<subclass name="DOL.Database.Arrow, GameServer" />
				<subclass name="DOL.Database.Bolt, GameServer" />
			</subclass>
      <subclass name="DOL.Database.MagicalStackableItem, GameServer">
        <property name="SpellID" type="Int32" not-null="true" column="`SpellID`" />
        <subclass name="DOL.Database.CatapultAmmunition, GameServer" />
        <subclass name="DOL.Database.Poison, GameServer" />
      </subclass>
		</subclass>
		<subclass name="DOL.Database.EquipableItem, GameServer">
			<property name="Quality" type="Byte" not-null="true" column="`Quality`" /> 
			<property name="Bonus" type="Byte" not-null="true" column="`Bonus`" /> 
			<property name="Durability" type="Byte" not-null="true" column="`Durability`" /> 
			<property name="Condition" type="Double" not-null="true" column="`Condition`" /> 
			<property name="MaterialLevel" not-null="true" column="`MaterialLevel`" /> 
			<property name="ProcSpellID" type="Int32" not-null="true" column="`ProcSpellID`" /> 
			<property name="ProcEffectType" not-null="true" column="`ProcEffectType`" /> 
			<property name="ChargeSpellID" type="Int32" not-null="true" column="`ChargeSpellID`" /> 
			<property name="ChargeEffectType" not-null="true" column="`ChargeEffectType`" /> 
			<property name="Charge" type="Byte" not-null="true" column="`Charge`" /> 
			<property name="MaxCharge" type="Byte" not-null="true" column="`MaxCharge`" /> 
			<set name="MagicalBonus" table="itemmagicalbonus" cascade="all">
				<key column="ItemID" />
				<composite-element class="DOL.Database.ItemMagicalBonus, GameServer">
					<property name="BonusType" not-null="true" column="`BonusType`"/>
					<property name="Bonus" type="Int16" not-null="true" column="`Bonus`"/>
				</composite-element>
			</set>
			<set name="AllowedClass" table="itemallowedclass" cascade="all">
				<key column="ItemID" />
				<element column="AllowedClass" type="DOL.Database.eCharacterClass, GameServer"/>
			</set>
			<subclass name="DOL.Database.Jewel, GameServer" />
			<subclass name="DOL.Database.Bracer, GameServer" />
			<subclass name="DOL.Database.Ring, GameServer" />
			<subclass name="DOL.Database.Waist, GameServer" />
			<subclass name="DOL.Database.Necklace, GameServer" >
				<subclass name="DOL.Database.TeleportationMedaillon, GameServer">
					<property name="X" type="Int32" not-null="true" column="`X`" /> 
					<property name="Y" type="Int32" not-null="true" column="`Y`" /> 
					<property name="Z" type="Int32" not-null="true" column="`Z`" /> 
					<property name="Heading" type="Int32" not-null="true" column="`Heading`" /> 
					<property name="Region" type="Int32" not-null="true" column="`Region`" /> 
				</subclass>
			</subclass>
			<subclass name="DOL.Database.VisibleEquipment, GameServer" >
				<property name="Color" type="Int32" not-null="true" column="`Color`" /> 
				<subclass name="DOL.Database.Cloak, GameServer" />
				<subclass name="DOL.Database.Instrument, GameServer">
					<property name="Type" not-null="true" column="`Type`" /> 
				</subclass>
				<subclass name="DOL.Database.Armor, GameServer">
					<property name="ArmorFactor" type="Byte" not-null="true" column="`ArmorFactor`" /> 
					<property name="ArmorLevel" not-null="true" column="`ArmorLevel`" />
					<property name="ModelExtension" type="Byte" not-null="true" column="`ModelExtension`" /> 
					<subclass name="DOL.Database.HeadArmor, GameServer" />
					<subclass name="DOL.Database.TorsoArmor, GameServer" />
					<subclass name="DOL.Database.HandsArmor, GameServer" />
					<subclass name="DOL.Database.FeetArmor, GameServer" />
					<subclass name="DOL.Database.LegsArmor, GameServer" />
					<subclass name="DOL.Database.ArmsArmor, GameServer" />
				</subclass>
				<subclass name="DOL.Database.Weapon, GameServer">
					<property name="DamagePerSecond" type="Byte" not-null="true" column="`DamagePerSecond`" /> 
					<property name="Speed" type="Int32" not-null="true" column="`Speed`" />
					<property name="DamageType" not-null="true" column="`DamageType`" />
					<property name="HandNeeded" not-null="true" column="`HandNeeded`" />
					<property name="GlowEffect" type="Int32" not-null="true" column="`GlowEffect`" />
					<subclass name="DOL.Database.Shield, GameServer">
						<property name="Size" not-null="true" column="`Size`" /> 
					</subclass>
					<subclass name="DOL.Database.RangedWeapon, GameServer">
						<property name="WeaponRange" type="Int32" not-null="true" column="`WeaponRange`" /> 
						<subclass name="DOL.Database.Compositebow, GameServer" />
						<subclass name="DOL.Database.Crossbow, GameServer" />
						<subclass name="DOL.Database.Longbow, GameServer" />
						<subclass name="DOL.Database.Recurvedbow, GameServer" />
						<subclass name="DOL.Database.Shortbow, GameServer" />
						<subclass name="DOL.Database.ThrownWeapon, GameServer" >
							<property name="MaxCount" type="Int32" not-null="true" column="`MaxCount`" />
							<property name="Count" type="Int32" not-null="true" column="`Count`" />
						</subclass>
					</subclass>
					<subclass name="DOL.Database.Axe, GameServer" />
					<subclass name="DOL.Database.Blades, GameServer" />
					<subclass name="DOL.Database.Blunt, GameServer" />
					<subclass name="DOL.Database.CelticSpear, GameServer" />
					<subclass name="DOL.Database.CrushingWeapon, GameServer" />
					<subclass name="DOL.Database.FlexibleWeapon, GameServer" />
					<subclass name="DOL.Database.Hammer, GameServer" />
					<subclass name="DOL.Database.HandToHand, GameServer" />
					<subclass name="DOL.Database.LargeWeapon, GameServer" />
					<subclass name="DOL.Database.LeftAxe, GameServer" />
					<subclass name="DOL.Database.Piercing, GameServer" />
					<subclass name="DOL.Database.PolearmWeapon, GameServer" />
					<subclass name="DOL.Database.Scythe, GameServer" />
					<subclass name="DOL.Database.SlashingWeapon, GameServer" />
					<subclass name="DOL.Database.Spear, GameServer" />
					<subclass name="DOL.Database.Staff, GameServer" />
					<subclass name="DOL.Database.Sword, GameServer" />
					<subclass name="DOL.Database.ThrustWeapon, GameServer" />
					<subclass name="DOL.Database.TwoHandedWeapon, GameServer" />
				</subclass>	
			</subclass>
		</subclass>
	</class>
</hibernate-mapping>
