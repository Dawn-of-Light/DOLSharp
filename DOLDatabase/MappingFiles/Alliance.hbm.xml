<?xml version="1.0" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.0">
	<class name="DOL.Database.Alliance, GameServer" table="`alliance`">
		<id name="AllianceID" column="`AllianceID`" unsaved-value="0">
			<generator class="native" />
		</id>
		<property name="AMotd" type="String(255)" not-null="true" column="`AMotd`"/>
		<bag name="AllianceGuilds" inverse="true" cascade="none" table="`guild`">
			<key column="Alliance" />
			<one-to-many class="DOL.Database.Guild, GameServer"/>
		</bag>
		<many-to-one name="AllianceLeader" unique="true" cascade="none" class="DOL.Database.Guild, GameServer" column="`AllianceLeader`"/>
	</class>
</hibernate-mapping>
