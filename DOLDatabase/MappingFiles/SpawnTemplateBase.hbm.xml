<?xml version="1.0" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.0">
	<class name="DOL.Database.SpawnGenerators.SpawnTemplateBase, GameServer" table="`spawntemplate`">
		<id name="SpawnTemplateBaseID" type="Int32" column="`SpawnTemplateBaseID`">
			<generator class="assigned" />
		</id>
		<discriminator column="SpawnTemplateBaseType"/> 

		<many-to-one name="MobTemplate" cascade="none" class="DOL.Database.GameMobTemplate, GameServer" column="`PersistantGameObjectID`"/>
	
		<subclass name="DOL.Database.SpawnGenerators.SpawnTemplateClassic, GameServer">
			<property name="Count" type="Int32" not-null="true" column="`Count`"/>
		</subclass>
		
	</class>
</hibernate-mapping>
