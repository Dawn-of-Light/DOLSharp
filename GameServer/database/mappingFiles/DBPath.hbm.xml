<?xml version="1.0" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.0">
	<class name="DOL.GS.Database.DBPath, GameServer" table="`Path`">
		<id name="PathID" column="`PathID`">
			<generator class="assigned" />
		</id>
		<property name="PathType" type="Int32" not-null="true" column="`PathType`"/>
		<array name="PathPoints" table="`PathPoint`">
			<key column="`PathID`"/>
			<index column="`Step`" type="Int32"/>
			<one-to-many class="DOL.GS.Database.DBPathPoint, GameServer"/>
		</array>
	</class>
</hibernate-mapping>
