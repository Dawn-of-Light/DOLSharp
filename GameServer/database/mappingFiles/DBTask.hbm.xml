<?xml version="1.0" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.0">
	<class name="DOL.GS.Database.DBTask, GameServer" table="`Task`">
		<id name="CharacterID" column="`CharacterID`" unsaved-value="0">
			<generator class="assigned" />
		</id>
		<property name="TimeOut" type="DateTime" not-null="true" column="`TimeOut`"/>
		<property name="TaskType" type="String(255)" not-null="true" column="`TaskType`"/>
		<property name="TasksDone" type="Int32" not-null="false" column="`TasksDone`"/>
		<property name="CustomPropertiesString" type="String(255)" not-null="true" column="`CustomPropertiesString`"/>
	</class>
</hibernate-mapping>
