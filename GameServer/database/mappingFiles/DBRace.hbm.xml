		<property name="ResistThrust" type="Int32" not-null="true" column="`ResistThrust`"/>
		<property name="ResistBody" type="Int32" not-null="true" column="`ResistBody`"/>
		<property name="ResistCold" type="Int32" not-null="true" column="`ResistCold`"/>
		<property name="ResistEnergy" type="Int32" not-null="true" column="`ResistEnergy`"/>
		<property name="ResistHeat" type="Int32" not-null="true" column="`ResistHeat`"/>
		<property name="ResistMatter" type="Int32" not-null="true" column="`ResistMatter`"/>
		<property name="ResistSpirit" type="Int32" not-null="true" column="`ResistSpirit`"/>
	</class>
</hibernate-mapping>
