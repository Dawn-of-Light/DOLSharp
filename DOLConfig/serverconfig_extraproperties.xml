<?xml version="1.0" standalone="yes"?>
<ExtraProperties>
  <!--
  Example:
  <Server>
    <property>PropertyName</property>
    <type>string|integer|boolean</type>
    <value>default value</value>
    <description>Property description</description>
  </Server>
  -->
  <Server>
    <property>InvalidNamesFile</property>
    <type>string</type>
    <value></value>
    <description>Location of the Invalid Names file</description>
  </Server>
  <Server>
    <property>CheatLoggerName</property>
    <type>string</type>
    <value>cheats</value>
    <description>Tag name for logging cheats</description>
  </Server>
  <Server>
    <property>GMActionLoggerName</property>
    <type>string</type>
    <value>gmactions</value>
    <description>Tag name for loggin GM actions</description>
  </Server>
  <Server>
    <property>CpuUse</property>
    <type>integer</type>
    <value>1</value>
    <description>How many CPUs will be used</description>
  </Server>
  <Server>
    <property>ZoneConfigFile</property>
    <type>string</type>
    <value></value>
    <description>Location of Zone config file</description>
  </Server>
  <Server>
    <property>RegionConfigFile</property>
    <type>string</type>
    <value></value>
    <description>Location of Region config file</description>
  </Server>
  <Server>
    <property>ScriptCompilationTarget</property>
    <type>string</type>
    <value></value>
    <description>Location of Script compilation target file</description>
  </Server>
  <Server>
    <property>LogConfigFile</property>
    <type>string</type>
    <value></value>
    <description>Location of Log config file</description>
  </Server>
  <Server>
    <property>ScriptAssemblies</property>
    <type>string</type>
    <value></value>
    <description>List of Script assembies</description>
  </Server>
  <Server>
    <property>EnableUPnP</property>
    <type>boolean</type>
    <value>true</value>
    <description>Enables or disables Universal Plug-and-Play</description>
  </Server>
</ExtraProperties>